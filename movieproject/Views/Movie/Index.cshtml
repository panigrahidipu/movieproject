@model List<movieproject.Models.Movie>

<h2>Movies</h2>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Release Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.ReleaseDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>

@Html.ActionLink("Create New", "Create")
